#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    long long int n;
     scanf("%lld",&n);
    if(n==1)
     printf("Neither prime nor Composite");

else{
    for(int i=2;i<=sqrt(n);i++){
        if(n%i==0){
          printf("No");
          return 0;
    }
    }
     printf("Yes");
}
     return 0;

    }
   
    

