#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    
    double  P,Q,R,S;
    scanf("%lf %lf %lf %lf",&P,&Q,&R,&S);
    if(P>(Q+R+S))
        printf("YES");
    else if(Q>(P+R+S))
        printf("YES");
    else if(R>(P+Q+S))
        printf("YES");
    else if(S>(P+Q+R))
        printf("YES");
    else
         printf("NO");   
    return 0;
}
